<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.athleticsgymapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AthleticsGymApp">
        <activity android:name=".TreninziActivity"></activity>
        <activity android:name=".OpcijeActivity" />
        <activity android:name=".PrijavaActivity" />
        <activity
            android:name=".ui.login.PrijavaActivity"
            android:label="@string/title_activity_prijava" />
        <activity android:name=".RegistracijaActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>